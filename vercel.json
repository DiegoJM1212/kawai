{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "manifest.json",
      "use": "@vercel/static"
    },
    {
      "src": "sw.js",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "controladores/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/adopcion/adopapi",
      "dest": "/controladores/adopapi.js"
    },
    {
      "src": "/banco central/centralapi",
      "dest": "/controladores/centralapi.js"
    },
    {
      "src": "/banco/bancoapi",
      "dest": "/controladores/bancoapi.js"
    },
    {
      "src": "/medicos/medicoapi",
      "dest": "/controladores/medicoapi.js"
    },
    {
      "src": "/paypal/paypalapi",
      "dest": "/controladores/paypalapi.js"
    },
    {
      "src": "/proteccion animal/proteapi",
      "dest": "/controladores/proteapi.js"
    },
    {
      "src": "/registro/registroapi",
      "dest": "/controladores/registroapi.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
